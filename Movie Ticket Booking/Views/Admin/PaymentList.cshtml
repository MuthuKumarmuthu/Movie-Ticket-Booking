@model IEnumerable<BusinessObject.PaymentObject>

@{
    ViewBag.Title = "PaymentList";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<br>
<h2>PaymentList</h2>
<br>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Paymentid)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Cardnumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nameoncard)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Expirydate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cvv)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Total)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Paymentid)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.Cardnumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nameoncard)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Expirydate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cvv)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Total)
        </td>
      
    </tr>
}

</table>
